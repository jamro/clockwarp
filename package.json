{
  "name": "clockwarp",
  "version": "1.0.3",
  "description": "JavaScript alternative to setTimeout offering time manipulation features",
  "main": "src/ClockWarp.js",
  "scripts": {
    "all": "npm run lint && npm run doc && npm run test",
    "lint": "eslint src/ClockWarp.js",
    "lint-fix": "eslint src/ClockWarp.js --fix",
    "test": "mocha src",
    "watch": "mocha src --watch --parallel --recursive",
    "doc": "jsdoc2md src/ClockWarp.js -t doc/README.hbs > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamro/clockwarp.git"
  },
  "author": "Krzysztof Jamroz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamro/clockwarp/issues"
  },
  "homepage": "https://github.com/jamro/clockwarp#readme",
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc-to-markdown": "^8.0.0",
    "mocha": "^10.2.0",
    "sinon": "^15.0.1"
  }
}
